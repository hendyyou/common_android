package com.digione.zgb2b.bean.product;

import java.io.Serializable;
import java.util.List;

/**
 * 订单级活动信息实体类
 * 
 * @author zhangqr
 * 
 */
public class ActOrderLevelBean implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4785138112311947722L;

	/**
	 * 订单级活动标题
	 */
	private String actOrderLevelTitle;

	/**
	 * 订单级活动提示列表
	 */
	private List<ActItemInfo> actOrderLevelList;

	public String getActOrderLevelTitle() {
		return actOrderLevelTitle;
	}

	public void setActOrderLevelTitle(String actOrderLevelTitle) {
		this.actOrderLevelTitle = actOrderLevelTitle;
	}

	public List<ActItemInfo> getActOrderLevelList() {
		return actOrderLevelList;
	}

	public void setActOrderLevelList(List<ActItemInfo> actOrderLevelList) {
		this.actOrderLevelList = actOrderLevelList;
	}

}
