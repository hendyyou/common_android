package com.digione.zgb2b.bean.product;

import java.io.Serializable;

/**
 * 主显示屏实体类
 * 
 * @author zhangqr
 * 
 */
public class ScreenBean implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 116155067723574897L;

	/**
	 * 屏幕尺寸ID
	 */
	private Integer screenSizeRangeId;

	/**
	 * 屏幕尺寸名称
	 */
	private String screenSizeRangeName;

	/**
	 * 屏幕尺寸最大的值
	 */
	private String maxScreenSize;

	/**
	 * 屏幕尺寸最小的值
	 */
	private String minScreenSize;

	/**
	 * 屏幕尺寸对应的产品列表URL
	 */
	private String screenSizeRangeSearchUrl;

	public Integer getScreenSizeRangeId() {
		return screenSizeRangeId;
	}

	public void setScreenSizeRangeId(Integer screenSizeRangeId) {
		this.screenSizeRangeId = screenSizeRangeId;
	}

	public String getScreenSizeRangeName() {
		return screenSizeRangeName;
	}

	public void setScreenSizeRangeName(String screenSizeRangeName) {
		this.screenSizeRangeName = screenSizeRangeName;
	}

	public String getMaxScreenSize() {
		return maxScreenSize;
	}

	public void setMaxScreenSize(String maxScreenSize) {
		this.maxScreenSize = maxScreenSize;
	}

	public String getMinScreenSize() {
		return minScreenSize;
	}

	public void setMinScreenSize(String minScreenSize) {
		this.minScreenSize = minScreenSize;
	}

	public String getScreenSizeRangeSearchUrl() {
		return screenSizeRangeSearchUrl;
	}

	public void setScreenSizeRangeSearchUrl(String screenSizeRangeSearchUrl) {
		this.screenSizeRangeSearchUrl = screenSizeRangeSearchUrl;
	}

}
